<Window x:Class="Proiect.ImageWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proiect"
        mc:Ignorable="d"
        Title="ImageWindow" Height="500" Width="400"
        Background="Black"
        Icon="pack://application:,,,/Artemis .ico">
    <Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Right" Margin="0,0,200,0">
            <Image x:Name="FullSizeImage" Stretch="Uniform" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </StackPanel>
        <Button Content="Back" Click="BackButton_Click" Margin="310,444,10,10"/>
    </Grid>
</Window>
